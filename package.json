{
  "name": "next-start-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-logger": "NODE_OPTIONS='-r next-logger' FORCE_COLOR=true next | pino-pretty -t -c -s \"level > '30'\" next dev",
    "env-dev": "env-cmd -f .env.development next dev",
    "env-local": "env-cmd -f .env.local next dev",
    "env-prod": "env-cmd -f .env.production next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "precommit": "echo 'pre-commit checks...' && exit 0",
    "release": "standard-version"
  },
  "pre-commit": [
    "precommit",
    "lint"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "@tanstack/query-core": "^5.59.13",
    "@tanstack/react-query": "^5.59.15",
    "@tanstack/react-query-devtools": "^5.59.15",
    "@tanstack/react-query-next-experimental": "^5.59.15",
    "axios": "^1.7.7",
    "cookies-next": "^4.3.0",
    "dayjs": "^1.11.13",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.15",
    "next-client-cookies": "^1.1.1",
    "next-international": "^1.2.4",
    "next-logger": "^5.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "autoprefixer": "^10.4.20",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.3.0",
    "pino-smart": "^0.0.9",
    "postcss": "^8.4.47",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
